# ES6 snippets

# Arrow function
snippet =>
	(${1}) => {
		${2}
	}

# For of loop
snippet forof
	for (let ${1:s} of ${2:sequence}) {
		${3}
	}

# Import
snippet im
	import ${1:foo} from '${2:bar}'

# React component
snippet comp
	import React from 'react';

	export default React.createClass({
  		render() {
    			return (
    				${1:foo}
    			);
  		}
	});
